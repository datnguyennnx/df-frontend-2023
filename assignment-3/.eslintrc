{
  "extends": [
    "./node_modules/@dwarvesf/react-eslint-config",
    "plugin:storybook/recommended"
  ],
  "globals": {
    "tw": true,
    "chrome": true
  },
  "parserOptions": {
    "ecmaVersion": 10,
    "ecmaFeatures": {
      "jsx": true
    },
    "project": ["./tsconfig.json"],
    "createDefaultProgram": true
  },
  "rules": {},
  "overrides": [
    {
      "files": ["**/*.ts?(x)", "**/*.js?(x)"]
    }
  ]
}